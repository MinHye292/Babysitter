<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cat.mapper.SitterMapper">
    <insert id="insertSitter">
        insert into sitter
            (
                ctpvNm,childCareInstNm,rprsTelno,drtlnTelno,fxno,zip,addr,lot,lat,dataCrtrYmd
            )
            VALUES
            (
                #{ctpvNm},#{childCareInstNm},#{rprsTelno},#{drtlnTelno},
                #{fxno},#{zip},#{addr},#{lot},#{lat},#{dataCrtrYmd}
            );
    </insert>
    <select id="selectMap" resultType="cat.vo.SitterVO.MapInfoVO">
        select ctpvNm,childCareInstNm,rprsTelno,drtlnTelno,fxno,zip,addr,lot,lat
        from sitter where seq=#{seq}
    </select>
    <select id="selectProvider" resultType="cat.vo.SitterVO.ProviderVO">
        select seq,childCareInstNm,addr
        from sitter
    </select>
</mapper>